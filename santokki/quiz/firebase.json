{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      ".firebaserc",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/dashboard.html",
        "destination": "/dashboard.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
